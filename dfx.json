{
  "canisters": {
    "system_api": {
      "candid": "src/icpass/system_api/vetkd_system_api.did",
      "package": "vetkd_system_api",
      "type": "rust"
    },
    "icpass": {
      "main": "src/icpass/backend/main.mo",
      "type": "motoko"
    },
    "icpass_assets": {
      "dependencies": ["icpass"],
      "frontend": {
        "entrypoint": "src/icpass/public/main.js"
      },
      "type": "assets",
      "source": [
        "src/icpass/public",
        "dist/icpass_assets"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "tungsten": {
      "providers": ["https://gw.dfinity.network"],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:9090",
      "type": "ephemeral"
    }
  },
  "dfx": "0.15.0"
}
